@model TP4.Models.UsuarioCajaDeAhorro

@{
    ViewData["Title"] = "Delete";
}

<h1>Eliminar</h1>

<h4>Esta seguro de eliminar la asociación?</h4>
<div>
    <h4>UsuarioCajaDeAhorro</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.caja)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.caja._cbu)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.user)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.user._id_usuario)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="_id_caja" />
        <input type="hidden" asp-for="_id_usuario" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> 

    </form>
    <div>
        <a asp-action="Index" asp-route-id="@ViewBag.id">Back to List</a>
    </div>
</div>
